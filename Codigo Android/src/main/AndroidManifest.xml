<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.reacompany.reaproyecto">

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:vmSafeMode="true">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FactoryActivity"
            android:label="@string/Factory" />
        <activity
            android:name=".PrototypeActivity"
            android:label="@string/Prototype" />
        <activity
            android:name=".SingletonActivity"
            android:label="@string/Singleton" />
        <activity
            android:name=".StateActivity"
            android:label="@string/State" />
        <activity android:name=".CarcelActivity" />
        <activity android:name=".WindowsActivity" />
        <activity android:name=".CervezaActivity" />
        <activity android:name=".CalculadoraActivity" />
        <activity android:name=".LuchadorActivity" />
        <activity android:name=".BatmanActivity" />
        <activity android:name=".LagoActivity" />
        <activity android:name=".LuzActivity" />
        <activity android:name=".NaranjaActivity" />
        <activity
            android:name=".ProfeActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_profe"
            android:theme="@style/FullscreenTheme"></activity>
    </application>

</manifest>
