<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.reacompany.reaproyecto.MainActivity"
    android:background="@android:color/holo_orange_dark"
    android:id="@+id/ReaProyecto">

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="160dp"
        android:layout_height="80dp"
        android:text="Factory"
        android:id="@+id/FactoryButton"
        android:background="#d0d0d0"
        android:textSize="25dp"
        android:onClick="lanzarFactory"
        android:textAlignment="center"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="70dp"
        android:layout_height="80dp"
        android:text="Prototype"
        android:id="@+id/PrototypeButton"
        android:background="@drawable/borre"
        android:textSize="10dp"
        android:onClick="lanzarPrototype"
        android:textAlignment="center"
        android:layout_alignTop="@+id/button7"
        android:layout_alignParentStart="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="70dp"
        android:layout_height="80dp"
        android:text="@string/State2"
        android:id="@+id/MementoButton"
        android:onClick="lanzarState2"
        android:background="@drawable/carcel"
        android:textSize="20dp"
        android:textAlignment="center"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/FactoryButton" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="70dp"
        android:layout_height="80dp"
        android:text="@string/State3"
        android:id="@+id/button"
        android:onClick="lanzarState3"
        android:background="@drawable/lago"
        android:textSize="20dp"
        android:textAlignment="center"
        android:layout_alignParentBottom="true"
        android:layout_alignEnd="@+id/FactoryButton" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="70dp"
        android:layout_height="80dp"
        android:text="@string/State"
        android:id="@+id/button2"
        android:onClick="lanzarState"
        android:background="@drawable/carne"
        android:textSize="20dp"
        android:textAlignment="center"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="70dp"
        android:layout_height="80dp"
        android:text="@string/State4"
        android:id="@+id/button3"
        android:onClick="lanzarState4"
        android:textSize="20dp"
        android:textAlignment="center"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/button"
        android:background="@drawable/luz" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="70dp"
        android:layout_height="80dp"
        android:text="Prototipe4"
        android:id="@+id/button4"
        android:background="@drawable/naranja"
        android:textSize="10dp"
        android:onClick="lanzarPrototype4"
        android:textAlignment="center"
        android:layout_alignTop="@+id/button6"
        android:layout_alignStart="@+id/button3" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="10dp"
        android:layout_height="80dp"
        android:text="Rea"
        android:id="@+id/button5"
        android:background="#d0d0d0"
        android:textSize="10dp"
        android:onClick="lanzarProfe"
        android:textAlignment="center"
        android:layout_centerVertical="true"
        android:layout_alignStart="@+id/button6" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="70dp"
        android:layout_height="80dp"
        android:text="Prototype3"
        android:id="@+id/button6"
        android:background="@drawable/batman"
        android:textSize="10dp"
        android:onClick="lanzarPrototype3"
        android:textAlignment="center"
        android:layout_below="@+id/button5"
        android:layout_alignStart="@+id/button" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="70dp"
        android:layout_height="80dp"
        android:text="Prototype2"
        android:id="@+id/button7"
        android:textSize="10dp"
        android:onClick="lanzarPrototype2"
        android:textAlignment="center"
        android:layout_below="@+id/button5"
        android:layout_alignStart="@+id/MementoButton"
        android:background="@drawable/luc" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="70dp"
        android:layout_height="80dp"
        android:text="Singleton3"
        android:id="@+id/button8"
        android:background="@drawable/cerveza"
        android:textSize="10dp"
        android:onClick="lanzarSingleton3"
        android:textAlignment="center"
        android:layout_alignTop="@+id/button9"
        android:layout_alignEnd="@+id/FactoryButton" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="70dp"
        android:layout_height="80dp"
        android:text="Singleton2"
        android:id="@+id/button9"
        android:background="@drawable/windows"
        android:textSize="10dp"
        android:onClick="lanzarSingleton2"
        android:textAlignment="center"
        android:layout_alignTop="@+id/button10"
        android:layout_alignStart="@+id/FactoryButton" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="."
        android:id="@+id/textView3"
        android:layout_alignEnd="@+id/button4"
        android:layout_marginTop="42dp"
        android:layout_below="@+id/PrototypeButton" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="70dp"
        android:layout_height="80dp"
        android:text="Singleton"
        android:id="@+id/button10"
        android:background="@mipmap/burro"
        android:textSize="10dp"
        android:onClick="lanzarSingleton"
        android:textAlignment="center"
        android:layout_above="@+id/button5"
        android:layout_alignParentStart="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="70dp"
        android:layout_height="80dp"
        android:text="Singleton4"
        android:id="@+id/button11"
        android:background="@drawable/calculadora"
        android:textSize="10dp"
        android:onClick="lanzarSingleton4"
        android:textAlignment="center"
        android:layout_above="@+id/button5"
        android:layout_toStartOf="@+id/textView3"
        android:nestedScrollingEnabled="false" />

</RelativeLayout>
